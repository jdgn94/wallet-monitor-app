CREATE TABLE banks (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL UNIQUE,
    image TEXT DEFAULT NULL
);

getAll:
SELECT * FROM banks;

insert:
INSERT INTO
    banks(name, image)
VALUES
    (?, ?);

update:
UPDATE
    banks
SET
    name = ?,
    image = ?
WHERE
    id = ?;

search:
SELECT * FROM banks WHERE LOWER(name) LIKE LOWER(?);